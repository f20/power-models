#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_m-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
cd _$RUN.tmp

perl ../run/make.pl -sqlite ../ModelM/Toys-2014/%-withEDCM-postDCP118.yml ../ModelM/Toys-2014/%-withEDCM-postDCP118-avgunitmeav.yml ../ModelM/Data-2014-02/*
perl ../run/export.pl tscs

cd ..
mv _$RUN.tmp $RUN.tmp
