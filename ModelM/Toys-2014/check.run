#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_m-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
cd _$RUN.tmp

../run/make.pl -sqlite ../ModelM/Data-2014-02/* ../ModelM/Current/%-*.yml ../ModelM/Toys-2014/%-postDCP118-EDCM.yml
../run/export.pl tscs

cd ..
mv _$RUN.tmp $RUN.tmp
