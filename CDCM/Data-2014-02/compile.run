#!/bin/sh
cd "`dirname \"$0\"`"
while [ ! -e pmod.pl ]; do pwd; cd ..; done
RUN=_cdcm2014-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
pushd _$RUN.tmp
perl ../pmod.pl -sqlite -stats -checksum ../CDCM/Current/%-after163.yml ../CDCM/Data-2014-02/*
perl ../pmod.pl all 10 22 31 37 38 39 40
popd
mv _$RUN.tmp $RUN.tmp
echo "$RUN.tmp complete; press any key to exit"
read
