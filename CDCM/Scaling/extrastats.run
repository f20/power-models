#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e pmod.pl ]; do pwd; cd ..; done
RUN=_dcp123stats-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
cd _$RUN.tmp
mkdir '~$models'

perl ../pmod.pl -sqlite ../CDCM/Data-2014-02/* ../CDCM/Scaling/%+after163-extrastats.yml ../CDCM/Scaling/%+dcp123-extrastats.yml
perl ../pmod.pl dcp123
perl ../pmod.pl tscs 40

cd ..
mv _$RUN.tmp $RUN.tmp
