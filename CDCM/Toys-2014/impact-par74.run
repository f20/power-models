#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_par74-$$
rm -rf _$RUN
mkdir _$RUN
cd _$RUN
mkdir '~$models'

../run/make.pl -sqlite ../CDCM/Current/%-post132par74.yml ../CDCM/Current/%-clean132.yml ../CDCM/Data-2014-02/*
../run/export.pl -dcp=post132par74 -name='paragraph 74 compliance'
