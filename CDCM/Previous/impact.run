#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_CP-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
cd _$RUN.tmp
mkdir ~\$models

perl ../run/make.pl -sqlite ../CDCM/Current/%-after163.yml ../CDCM/Data-2013-02/* ../CDCM/Data-2014-02/*
perl ../run/export.pl -dcp=2014-02 -base=2013-02
perl ../run/export.pl tscs

cd ..
mv _$RUN.tmp $RUN.tmp
