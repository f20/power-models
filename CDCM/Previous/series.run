#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_TS-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
cd _$RUN.tmp

time ../run/make.pl -pickbest ../CDCM/*/%-mini*.yml ../CDCM/Data-*-02/*
time ../run/import.pl -calc -sqlite *.xlsx
time ../run/export.pl tscs

cd ..
mv _$RUN.tmp $RUN.tmp
