#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_TS-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
cd _$RUN.tmp
time ../run/make.pl -pickbest -stats -sqlite ../CDCM/*/%-clean100.yml  ../CDCM/*/%-clean132.yml  ../CDCM/*/%-after163.yml ../CDCM/Data-*-02/*
time ../run/export.pl tscs
cd ..
mv _$RUN.tmp $RUN.tmp
