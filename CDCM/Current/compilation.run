#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_compil-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
pushd _$RUN.tmp
time ../run/make.pl ../CDCM/Current/%-after163.yml ../CDCM/Data-2014-02/*
time ../run/import.pl -convert -sqlite *.xl*
time ../run/export.pl all 10 37 38 39
time ../run/export.pl tscs
popd
mv _$RUN.tmp $RUN.tmp
echo 'Press any key to exit'
read
