#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_cdcm-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
pushd _$RUN.tmp
time ../run/make.pl -sqlite ../CDCM/Current/%-extras163.yml ../CDCM/Data-2014-02/*
time ../run/export.pl all 10 22 37 38 39 40
# time ../run/export.pl tscs
popd
mv _$RUN.tmp $RUN.tmp
echo 'Press any key to exit'
read
