#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_dcp133-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
cd _$RUN.tmp

../run/make.pl -sqlite -stats ../CDCM/Data-2014-02/* ../CDCM/DCP-133/%-*.yml ../CDCM/Current/%-after163.yml
../run/export.pl dcp133
../run/export.pl tscs 40

cd ..
mv _$RUN.tmp $RUN.tmp
