#!/bin/sh
cd "`dirname \"$0\"`"
while [ ! -e run/make.pl ]; do pwd; cd ..; done
RUN=_cdcm2013-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
pushd _$RUN.tmp
perl ../run/make.pl -sqlite -stats -checksum ../CDCM/Current/%-after163.yml ../CDCM/Data-2013-02/*
perl ../run/export.pl all 10 22 31 37 38 39 40
popd
mv _$RUN.tmp $RUN.tmp
echo "$RUN.tmp complete; press any key to exit"
read
