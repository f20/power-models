#!/bin/bash
cd "`dirname \"$0\"`"
while [ ! -e pmod.pl ]; do pwd; cd ..; done
RUN=_dcp179jun-$$
rm -rf _$RUN.tmp
mkdir _$RUN.tmp
cd _$RUN.tmp
mkdir '~$models'

perl ../pmod.pl -stats ../CDCM/DCP-179/%-dcp179apr-ctables.yml ../CDCM/Current/%-clean132.yml ../CDCM/Data-2014-02/*
perl ../pmod.pl -calc -sqlite '~$models'/*.xl*
perl ../pmod.pl -tscs 37 40

cd ..
mv _$RUN.tmp $RUN.tmp
